{
  "manifest_version": 2,
  "name": "Title",
  "description": "Description",
  "author": "Handsome.is",
  "homepage_url": "Homepage",
  "version": "0.0.0",
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "https://trello.com/",
    "https://api.airtable.com/"
  ],
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "https://trello.com/*"
      ],
      "css": [
        "css/style.css"
      ],
      "js": [
        "js/vendor/jquery.min.js",
        "js/vendor/jquery-ui.min.js",
        "js/config.js",
        "js/settings.js",
        "js/helpers.js",
        "js/pop-over.js",
        "js/notification.js",
        "js/api.js",
        "js/plugins/inheritance.js",
        "js/init.js"
      ]
    }
  ]
}
